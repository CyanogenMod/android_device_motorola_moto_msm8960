type hw_revs, domain;
type hw_revs_exec, exec_type, file_type;
init_daemon_domain(hw_revs)

allow hw_revs devpts:chr_file { read write ioctl };
allow hw_revs shell_exec:file read;
allow hw_revs system_data_file:dir { read open setattr };
allow hw_revs system_data_file:file { write open append setattr };
allow hw_revs system_file:file execute_no_trans;

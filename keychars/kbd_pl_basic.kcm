# Polish (Programmers) layout

type FULL

### Basic QWERTY keys ###

key A {
    label:                              'A'
    base:                               'a'
    shift, capslock:                    'A'
    ctrl, alt, meta:                    none
    ralt:                               '\u0105'
    shift+ralt:                         '\u0104'
    capslock+shift:                     'a'
}


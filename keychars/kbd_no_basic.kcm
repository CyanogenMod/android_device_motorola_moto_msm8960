    label, number:                      '4'
    base:                               '4'
    shift, capslock:                    '\u00a4'
    fn, alt, ctrl, meta:                '\u0024'
}

key 5 {
    label, number:                      '5'
    base:                               '5'
    shift, capslock:                    '%'
    fn, alt, ctrl, meta:                none
}


    label, number:                      '0'
    base:                               '0'
    shift, capslock:                     '='
    fn, ctrl, alt, meta:                '}'
}

key 1 {
    label, number:                      '1'
    base:                               '1'
    shift, capslock:                    '!'
    fn, ctrl, alt, meta:                none
}

key 2 {
    label, number:                      '2'
    base:                               '2'
    shift, capslock:                    '\u0022'
    fn, ctrl, alt, meta:                '@'
